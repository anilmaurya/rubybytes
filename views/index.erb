<div id="container">

  <div class='container'>
    <div class="page-header" id='banner'>
      <div class="row">
        <div class="col-lg-8 col-md-7 col-sm-6">
          <h1>Rubybytes</h1>
          <p class="lead">Ruby Dissassembler</p>
        </div>
      </div>
    </div>
  </div>

  <div id="main">
    <div class="console-container-full">
      <div class="form-group">
        <label for="rubyVersion" class="control-label">Ruby Version</label>
        <select class="form-control" id="rubyVersion">
          <option value="ruby_21">2.1</option>
          <option value="ruby_22">2.2</option>
          <option value="ruby_23">2.3</option>
        </select>
      </div>
    </div>
    <div class="console-container">
      Ruby Code
      <div class="console">
        <textarea id="inputTextArea">puts 'hi'</textarea>
      </div>
      <a id="disassemble_button" href="#" class="btn" onClick="codebytes.submitCode(); return false;">Disassemble</a>
    </div>
    <div class="console-container">
      Disassembled Code
      <div class="console">
        <textarea id="outputTextArea"></textarea>
      </div>
    </div>
  </div>

</div>

